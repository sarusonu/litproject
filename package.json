{
  "name": "litproject",
  "version": "1.0.0",
  "description": "Lit project using babel, rollup, eslint and karma",
  "scripts": {
    "build": "babel src -d lib",
    "test": "karma start karma.conf.cjs",
    "lint": "eslint ./src/*.js",
    "serve": "es-dev-server --node-resolve --watch --port 3003",
    "bundle": "npm run build && rollup -c"
  },
  "dependencies": {
    "lit-element": "^2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.2",
    "es-dev-server": "^1.46.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.13.0",
    "lit-analyzer": "^1.1.9",
    "web-component-analyzer": "^1.0.3",
    "@rollup/plugin-replace": "^2.3.1",
    "rollup": "^1.32.1",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "@open-wc/testing": "^2.5.10",
    "@open-wc/testing-karma": "^3.3.11",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^7.1.1",
    "deepmerge": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarusonu/litproject.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sarusonu/litproject/issues"
  },
  "homepage": "https://github.com/sarusonu/litproject#readme"
}
